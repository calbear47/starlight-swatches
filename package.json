{
	"name": "js-package-starter",
	"version": "1.0.0",
	"description": "Template repo to get started on a JS project with Jest tests and Webpack build system & linting",
	"main": "dist/index.js",
	"scripts": {
		"build": "webpack --mode=production",
		"format": "npm run prettier -- --write",
		"lint": "eslint src",
		"prettier": "prettier \"**/*.+(js|json|yml|yaml|css|scss|ts|md|mdx)\"",
		"precommit": "npm run validate",
		"test": "jest --coverage",
		"validate": "npm run lint && npm run prettier -- --list-different && npm build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/calbear47/js-package-starter.git"
	},
	"author": "Kyle Kirst",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/calbear47/js-package-starter/issues"
	},
	"homepage": "https://github.com/calbear47/js-package-starter#readme",
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.4",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/preset-env": "^7.4.5",
		"babel-loader": "^8.0.6",
		"babel-plugin-emotion": "^10.0.14",
		"css-loader": "^3.0.0",
		"eslint": "^6.0.1",
		"eslint-config-prettier": "^6.0.0",
		"file-loader": "^4.0.0",
		"husky": "^3.0.0",
		"jest": "^24.8.0",
		"lint-staged": "^9.0.1",
		"mini-css-extract-plugin": "^0.7.0",
		"node-sass": "^4.12.0",
		"prettier": "^1.18.2",
		"sass-loader": "^7.1.0",
		"webpack": "^4.35.2",
		"webpack-cli": "^3.3.5"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"lodash.isnil": "^4.0.0",
		"lodash.isplainobject": "^4.0.6"
	}
}
